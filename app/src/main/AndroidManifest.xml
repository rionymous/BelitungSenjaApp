        <activity android:name=".Tiket" />
        <activity android:name=".SuccessBooking" />
        <activity android:name=".Booking" />
        <activity android:name=".InfoPaket" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".SignUpSuccess" />
        <activity
            android:name=".SignUpTwo"
            android:parentActivityName=".SignUpOne" />
        <activity
            android:name=".SignUpOne"
            android:parentActivityName=".GetStarted" />
        <activity
            android:name=".SignIn"
            android:parentActivityName=".GetStarted" />
        <activity android:name=".GetStarted" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>